@{
    ViewData["Title"] = "Order Pizza";
    Layout = "_Layout";
}
}
@model HomePageViewModel
@await Html.PartialAsync("../Shared/NavHomePartial.cshtml")
<div class="order-section scrollable js--order-section">
    <div class="row" style="margin-top: 25px;">
        <div class="form-group col-md-4 offset-md-4">
            <h3 style="font-size: 36px; font-weight: 700; text-align: center">Place an order</h3>
        </div>
    </div>
    <div id='calendar'></div>
</div>
@section Scripts
    {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js"></script>
    <link href="~/lib/calendar/fullcalendar.css" rel="stylesheet" />
    <script src="~/lib/calendar/fullcalendar.js"></script>
    <script src="~/js/site.js"></script>
}